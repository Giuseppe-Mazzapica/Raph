{
    "name": "gmazzap/raph",
    "description": "WordPress plugin that renders shortcodes in posts content.",
    "keywords": [
        "wordpress"
    ],
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/Raph/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/Raph"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "Raph\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
